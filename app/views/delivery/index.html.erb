<div class="textsearchbox texts" style="display:none;" id="dn-search-box">
	<span class="searchcontenttype">运单号</span>
	<input type="text"
	name="searchcontent" class="searchcontent" placeholder="请输入运单号" id="dn-search-text"/>
	<input
	type="button" name="startsearch" class="startsearch" value="搜索" />
</div>
<div class="textsearchbox texts" style="display:none;"  id="rece-search-box">
	<span class="searchcontenttype">客户号</span>
