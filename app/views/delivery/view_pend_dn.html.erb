<link rel="stylesheet"  type="text/css" href="/assets/delivery/supWaybill.css" />
<script src="/assets/deliery-send-receive.js" type="text/javascript"></script>
<%if @msg.result%>
<div class="fillindestinations">
     <p>
          运单发送预览数据: <%= @msg.object.key%>
     </p>
     <span class="errparts" style="display:none;">*请先填写正确的送货地址</span></br>
          <input type="hidden" value="<%= @msg.object.key%>" id="dnkey-hidden">
     <div class="fillind">
          <input type="text" class="fillindest"id="client-text" autocomplete="off" disabled="false"
          value="<%= Organisation.find_by_id(@msg.object.orgId).search_client_byId(@msg.object.desiOrgId.to_i)%>"
          disabled/>
          <input type="text" class="fillindest fillindestlonger" placeholder="请您填写送货地址-必填" id="destination-text" value="Leoni">
     </div>
</div>
<div id='delivery-items-div'>
     <%= render :partial=>'dn_pend_items',:locals=>{:dnItems=>@msg.object.items}%>
</div>
<div class="botbts">
     <div class="btsave botbtn">
          <p>
               打印标签
          </p>
     </div>
     <div class="btcancel botbtn" id="cancelDeliverySendBtn" onclick="cancel_staff_dn();">
          <p>
               取消
          </p>
     </div>
     <div class="btsend botbtn" onclick="send_staff_dn(this);">
          <p>
               发送
          </p>
     </div>
</div>
<%= render :partial => "shared/handling_msg_box", :locals => { :handlemsg => "正在发送中..." } %>
<%else%>
<div class="abstractblock">
     <%= @msg.content%>
</div>
<%end%>