		<div class="chartview" style="display: none">
			<div class="adjustbt previousdate" startline="null">
			</div>
			<div class="centerchart" prime="null" startline="null">
					<div class="closebt" onclick="chartview_cancel();"></div>
					<div class="charttypebt charttypelinea chartactive" name="line" style="left:10%"></div>
					<div class="charttypebt charttypestepb" name="step" style="left:16%"></div>
					<div id="charting" style="margin:auto; top:20%; width:600px;height:300px"></div>
			</div>
			<div class="adjustbt laterdate" startline="null">
			</div>
